# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

[*.js]
charset = utf-8
indent_style = space
indent_size = 2
max_line_length = 100

[{*.json,.travis.yml,.eslintrc,.babelrc}]
indent_style = space
indent_size = 2
